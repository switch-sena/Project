@model IEnumerable<ConsumeSwitch.Models.Usuarios>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdUsua)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreUsua)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsua)
            </td>
            <td>

                @Html.DisplayFor(modelItem => item.NombreUsua)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdUsua }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdUsua }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.IdUsua })
            </td>
        </tr>
    }

</table>
